        <content approved="true" contenttype="Community-Blog"
            contenttypekey="10065" created="2007-10-21 14:40"
            current="true" key="26362" languagecode="en"
            languagekey="0" priority="0"
            publishfrom="2007-10-21 14:40" state="5" status="2"
            timestamp="2008-05-19 22:57" unitkey="106" versionkey="54566">
            <owner deleted="false"
