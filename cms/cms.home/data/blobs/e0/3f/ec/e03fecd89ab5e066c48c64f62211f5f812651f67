$(function() {

    // Tooltip
    $('.tooltip').tooltip({
        track: true,
        delay: 0,
        showURL: false,
        showBody: ' - ',
        fade: 250
    });
              
});
$(document).ready(function() {
  $('pre[type=syntaxhighlighter]').each(function() {
    var content = $(this).html();
    $(this).html(content);
  });
});